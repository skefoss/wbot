{
    "name": "wbot/wbot",
    "description": "Fetch/Serve files over the web from a local server",
    "homepage": "https://packagist.org/packages/ske/wbot",
    "license": "Apache-2.0",
    "keywords": [
        "http-server",
        "http-client",
		"fetch",
		"serve",
        "www-robot",
		"wget"
    ],
    "scripts": {
        "fetch": "./wbot fetch",
        "serve": "./wbot serve"
    },
    "authors": [{
        "name": "SIGUI KessÃ© Emmanuel",
        "homepage": "https://dev.sikessem.com",
        "email": "contact@sikessem.com",
        "role": "Developer"
    }],
    "support": {
        "source": "https://github.com/skefoss/wbot",
        "issues": "https://github.com/skefoss/wbot/issues"
    },
    "require": {
        "php": ">=8.0",
        "symfony/console": "^6.0"
    },
  	"autoload": {
	  	"psr-4": {
		  	"Ske\\Bot\\Application\\": "src/app/",
		  	"Ske\\Bot\\Command\\": "src/cmd/"
		}
	},
    "config": {
        "web-dir": "res",
        "vendor-dir": "vendor",
        "preferred-install": "dist",
        "sort-packages": true
    },
    "bin": "./wbot",
    "minimum-stability": "dev",
    "prefer-stable": true
}
